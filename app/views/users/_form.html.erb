<%= form_with(model: @user, url:'/signup', local: true) do |f|%>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %><br><br>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %><br><br>
    <%= f.label :email %>
    <%= f.email_field :email %><br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.password_field :password_confirmation %><br><br>
    <%= f.submit "Sign Up"%>
<% end %>